// LCM O(max(a,b))

#include<bits/stdc++.h>
using namespace std;

typedef long long ll;

ll LCM(ll a, ll b)
{
   int res= max(a,b);
   while(true){
   if(res%a ==0 && res%b ==0)
   {
     return res;
   }
     
     res++;}
   
   
    
}

int main()
{
    ll a, b;
    cin>>a>>b;
    
    cout<<LCM(a,b);
}
